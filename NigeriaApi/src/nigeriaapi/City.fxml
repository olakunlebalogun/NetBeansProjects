<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="545.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nigeriaapi.CityController">
   <children>
      <Label layoutX="150.0" layoutY="14.0" text="City Page">
         <font>
            <Font name="Segoe UI Bold" size="36.0" />
         </font></Label>
      <Label layoutX="74.0" layoutY="127.0" text="State">
         <font>
            <Font name="Segoe UI Bold" size="16.0" />
         </font></Label>
      <Label layoutX="74.0" layoutY="204.0" text="City">
         <font>
            <Font name="Segoe UI Bold" size="16.0" />
         </font></Label>
      <Label layoutX="69.0" layoutY="273.0" text="Local Govt">
         <font>
            <Font name="Segoe UI Bold" size="16.0" />
         </font></Label>
      <TextField fx:id="state" layoutX="170.0" layoutY="124.0" />
      <ComboBox fx:id="city" layoutX="169.0" layoutY="201.0" onAction="#cityComboClicked" prefWidth="150.0" />
      <ComboBox fx:id="lga" layoutX="169.0" layoutY="270.0" onAction="#lgaComboClicked" prefWidth="150.0" />
      <Button fx:id="add" layoutX="169.0" layoutY="357.0" mnemonicParsing="false" onAction="#addClicked" text="Add">
         <font>
            <Font name="Segoe UI Bold Italic" size="14.0" />
         </font></Button>
      <Button fx:id="edit" layoutX="273.0" layoutY="357.0" mnemonicParsing="false" onAction="#editClicked" text="Edit">
         <font>
            <Font name="Segoe UI Bold Italic" size="14.0" />
         </font></Button>
      <Button fx:id="delete" layoutX="213.0" layoutY="412.0" mnemonicParsing="false" onAction="#deleteClicked" text="Delete">
         <font>
            <Font name="Segoe UI Bold Italic" size="14.0" />
         </font></Button>
   </children>
</AnchorPane>
