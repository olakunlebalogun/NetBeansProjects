<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="twitter.FXMLDocumentController">
   <children>
      <VBox alignment="CENTER_LEFT" layoutX="201.0" layoutY="35.0" prefHeight="465.0" prefWidth="410.0" spacing="20.0">
         <children>
            <Label prefHeight="21.0" prefWidth="230.0" text="Choose your best player?">
               <font>
                  <Font name="Segoe UI Bold" size="18.0" />
               </font>
            </Label>
            <Button fx:id="leoButton" alignment="CENTER" contentDisplay="RIGHT" graphicTextGap="40.0" mnemonicParsing="false" onAction="#pushLeoButton" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 20;" text="Lionel Messi">
               <graphic>
                  <Label fx:id="leoLabel" graphicTextGap="10.0" text="Label" />
               </graphic>
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </Button>
            <Button fx:id="ronButton" alignment="CENTER" contentDisplay="RIGHT" graphicTextGap="20.0" mnemonicParsing="false" onAction="#pushRonButton" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 20;" text="Christiano Ronaldo">
               <graphic>
                  <Label fx:id="ronLabel" text="Label" />
               </graphic>
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </Button>
            <Button fx:id="neyButton" alignment="CENTER" contentDisplay="RIGHT" graphicTextGap="45.0" mnemonicParsing="false" onAction="#pushNeyButton" prefHeight="40.0" prefWidth="250.0" style="-fx-background-radius: 20;" text="Neymar Jr." textAlignment="CENTER">
               <graphic>
                  <Label fx:id="neyLabel" text="Label" />
               </graphic>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
